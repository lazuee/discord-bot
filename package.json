{
	"name": "@lazuee/discord-bot",
	"license": "MIT",
	"scripts": {
		"preinstall": "npx only-allow pnpm",
		"format": "prettier \"**/*.{cjs,mjs,js,jsx,ts,tsx}\" --write --ignore-unknown",
		"build": "tsc && tsc-alias -s",
		"dev": "set NODE_ENV=development && nodemon"
	},
	"dependencies": {
		"@discordjs/builders": "1.4.0",
		"captcha-canvas": "3.2.1",
		"chalk": "4.1.2",
		"discord.js": "14.7.1",
		"ora": "5.4.1",
		"type-fest": "3.4.0",
		"typed-regex": "0.0.8",
		"undici": "5.14.0"
	},
	"devDependencies": {
		"@trivago/prettier-plugin-sort-imports": "4.0.0",
		"@types/node": "18.11.12",
		"dotenv": "16.0.3",
		"nodemon": "2.0.20",
		"prettier": "2.8.1",
		"ts-node": "10.9.1",
		"tsc-alias": "1.8.2",
		"tsconfig-paths": "4.1.1",
		"tslib": "2.4.1",
		"typescript": "4.9.4"
	},
	"engines": {
		"node": ">=18.0.0",
		"pnpm": ">=7.18.0",
		"npm": "please-use-pnpm",
		"yarn": "please-use-pnpm"
	},
	"contributors": [
		"lazuee <lazuee.dev@gmail.com>"
	]
